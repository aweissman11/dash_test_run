{
  "name": "test_run",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build+start": "npm run build && npm run start",
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/tailwind/tailwind.css --minify",
    "build:remix": "remix build --sourcemap",
    "build:server": "sleep 1 && esbuild --platform=node --format=cjs ./server/* --outdir=build --sourcemap=external",
    "__build:server-comment__": "^ we sleep so the build:server finishes last",
    "build:strapi": "npm run build --prefix ./strapi",
    "dev": "run-p dev:*",
    "dev:strapi": "npm run develop --prefix ./strapi",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./mocks ./build/server.js",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/tailwind/tailwind.css --minify",
    "start": "run-p start:*",
    "start:server": "cross-env NODE_ENV=production node ./build/server.js",
    "start:strapi": "npm run start --prefix ./strapi",
    "storybook": "run-p dev:css storybook:start",
    "storybook:start": "start-storybook -h localhost -p 6006",
    "storybook:build": "build-storybook",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . && prettier -c .",
    "setup": "prisma migrate dev && prisma db seed",
    "test": "vitest",
    "percy": "percy",
    "pretest:e2e:run": "npm run build",
    "test:e2e": "npx playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e",
    "postinstall": "patch-package"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
